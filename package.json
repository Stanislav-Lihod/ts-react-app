{
  "name": "ts-react-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "serve": "webpack serve --env port=3000 ",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "test": "jest --config ./config/jest/jest.config.ts",
    "test-loki": "npx loki test",
    "test-loki:ok": "npx loki approve",
    "test-loki:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "sb": "storybook dev -p 6006 -c ./config/storybook",
    "sb:build": "storybook build -c ./config/storybook",
    "testLoki": "concurrently --kill-others --raw --success first \"npm run sb\" \"npm run test-loki\""
  },
